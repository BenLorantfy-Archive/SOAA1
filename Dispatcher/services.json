{
	"services":[
		{
			 "tagName":"CAR-LOAN"
			,"serviceName":"Car Loan Calculator"
			,"securityLevel":1
			,"description":"Calculates car loan based on a principle amount and an interest rate"
			,"path":"/calculations"
            ,"port":1337
            ,"start":"java -jar ../Services/CarLoanCalculator/dist/CarLoanCalculator.jar"
			,"arguments":[
				{
					  "name":"PrincipalAmount"
					 ,"mandatory":true
					 ,"type":"float"
				},
				{
					  "name":"InterestRate"
					 ,"mandatory":true
					 ,"type":"float"
				}
			]
			,"returns":[
				{
					 "name":"MonthlyPaymentFor36MonthLoan"
					,"type":"float"
				},
				{
					 "name":"MonthlyPaymentFor48MonthLoan"
					,"type":"float"
				},
				{
					 "name":"MonthlyPaymentFor60MonthLoan"
					,"type":"float"
				}
			]
		},
		{
			 "tagName":"GIORP-TOTAL"
			,"serviceName":"giorpTotaller"
			,"securityLevel":1
			,"description":"Calculates and returns the purchase sub-total, various tax additions and grand total for the purchase given the province/territory"
			,"path":"/"
            ,"port":1234
            ,"start":"Services/giorpTotaller/giorpTotaller/bin/Debug/giorpTotaller.exe"
			,"arguments":[
				{
					  "name":"PurchaseAmount"
					 ,"mandatory":true
					 ,"type":"float"
				},
				{
					  "name":"Province"
					 ,"mandatory":true
					 ,"type":"string"
				}
			]
			,"returns":[
				{
					 "name":"SubTotal"
					,"type":"float"
				},
				{
					 "name":"Gst"
					,"type":"float"
				},
				{
					 "name":"Pst"
					,"type":"float"
				},
				{
					 "name":"Hst"
					,"type":"float"
				},
				{
					 "name":"GrandTotal"
					,"type":"float"
				}
			]
		}
	]
}